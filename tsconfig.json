{
  "extend": "@tsconfig/create-react-app/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "src/*": ["src/*"],
      "types/*": ["src/types/*"]
    }
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
